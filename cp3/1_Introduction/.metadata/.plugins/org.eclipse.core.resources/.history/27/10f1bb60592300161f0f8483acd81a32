import java.util.*;
public class Main {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		String str = "";
		int n = input.nextInt();
		input.nextLine();
		for (int I = 1; I <= n; I++) {
			str = input.next();
			int lastIndex = str.length() - 1;
			if (str.equals("1") || str.equals("4") || str.equals("78")) {
				System.out.println("+");
			} else if (str.charAt(lastIndex - 1) == '3' && str.charAt(lastIndex) == '5') {
				System.out.println("-");
			} else if (str.charAt(0) == '9' && str.charAt(lastIndex) == '4') {
				System.out.println("*");
			} else {
				System.out.println("?");
			}
		}
		input.close();
	}

}
