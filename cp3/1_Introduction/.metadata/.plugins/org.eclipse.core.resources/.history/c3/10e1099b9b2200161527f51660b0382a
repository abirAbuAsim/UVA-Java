 * 	Verdict	: Accepted
 * 	OJ	 	: UVA online judge
 * 
 * 	@author: absakDev 
 * 	@version: 25/5/2016
 */
import java.util.*;
public class Main {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		String str = "";
		int counter = 0;
		while(input.hasNext()){
			counter++;
			str = input.next();
			if(str.equals("*"))
				break;
			
			if (str.equals("Hajj")){
				System.out.format("Case %d: Hajj-e-Akbar\n", counter);
			} else if(str.equals("Umrah")){
				System.out.format("Case %d: Hajj-e-Asghar\n", counter);
			}
		}
	}

}
